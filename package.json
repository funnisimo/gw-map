{
  "name": "gw-map",
  "version": "0.0.0",
  "description": "Map components for Roguelikes",
  "main": "dist/gw-map.cjs",
  "browser": "dist/gw-map.min.js",
  "module": "dist/gw-map.mjs",
  "types": "dist/gw-map.d.ts",
  "scripts": {
    "compile": "tsc --build tsconfig.json",
    "bundle": "rollup -c",
    "copy": "cp dist/gw-map.min.js docs/js",
    "build": "npm run compile && npm run bundle && npm run copy",
    "serve": "python -m SimpleHTTPServer",
    "test": "jest",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funnisimo/gw-map.git"
  },
  "keywords": [
    "roguelike",
    "map",
    "tile"
  ],
  "author": "Funnisimo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/funnisimo/gw-map/issues"
  },
  "homepage": "https://github.com/funnisimo/gw-map#readme",
  "devDependencies": {
    "@babel/types": "^7.12.11",
    "@jest/types": "^26.6.2",
    "gw-utils": "0.0.3",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "prettier": "^2.2.1",
    "rollup": "^2.35.1",
    "rollup-plugin-dts": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {},
  "peerDependencies": {
    "gw-utils": "^0.0.3",
    "gw-canvas": "^1.1.1"
  }
}
